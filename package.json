{
  "name": "connect-four",
  "version": "1.0.0",
  "description": "A Connect Four web app implemented in JS using Mithril",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.0.0"
  },
  "scripts": {
    "start": "node server/index.js",
    "dev": "node server/index.js",
    "build": "vite build",
    "preview": "NODE_ENV=production DISABLE_SSL=1 node server/index.js",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test": "playwright test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caleb531/connect-four.git"
  },
  "keywords": [
    "connect-four",
    "mithril"
  ],
  "author": "Caleb Evans",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/caleb531/connect-four/issues"
  },
  "homepage": "https://github.com/caleb531/connect-four#readme",
  "dependencies": {
    "@fontsource/ubuntu": "^5.2.5",
    "clsx": "^2.1.1",
    "compression": "^1.8.0",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "express-enforces-ssl": "^1.1.0",
    "helmet": "^8.1.0",
    "mithril": "^2.2.15",
    "moment": "^2.30.1",
    "open": "^10.1.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tiny-emitter": "^2.1.0",
    "underscore": "^1.13.7",
    "uuid": "^11.1.0",
    "vite": "^6.3.5",
    "vite-plugin-pwa": "^1.0.0",
    "workbox-window": "^7.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@playwright/test": "^1.52.0",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint__js": "^9.14.0",
    "@types/mithril": "^2.2.7",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "globals": "^16.1.0",
    "prettier": "^3.5.3",
    "sass-embedded": "^1.87.0",
    "sinon": "^20.0.0",
    "sinon-chai": "^4.0.0",
    "sinon-matchers": "^2.1.0",
    "workbox-build": "^7.3.0"
  },
  "pnpm": {
    "overrides": {
      "cookie@<0.7.0": ">=0.7.0"
