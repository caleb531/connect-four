{
  "name": "connect-four",
  "version": "1.0.0",
  "description": "A Connect Four web app implemented in JS using Mithril",
  "scripts": {
    "lint": "eslint app/scripts/**/*.js test/*.js",
    "test": "karma start --single-run",
    "coverage": "COVERAGE=true npm test && npm run remap-coverage && npm run coverage-report",
    "remap-coverage": "remap-istanbul -i coverage/coverage-unmapped.json -o coverage/coverage-remapped.json",
    "coverage-report": "./coverage-report.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caleb531/connect-four.git"
  },
  "keywords": [
    "connect-four",
    "mithril"
  ],
  "author": "Caleb Evans",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/caleb531/connect-four/issues"
  },
  "homepage": "https://github.com/caleb531/connect-four#readme",
  "dependencies": {
    "classnames": "^2.2.6",
    "fastclick": "^1.0.6",
    "mithril": "github:MithrilJS/mithril.js#deaf01c8e96446c97f31826b1d04ab4a4e757718",
    "tiny-emitter": "^1.1.0",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.0",
    "babel-brunch": "^6.1.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "brunch": "^2.10.17",
    "chai": "^3.5.0",
    "chai-dom": "^1.8.0",
    "copycat-brunch": "^1.1.0",
    "eslint": "^4.19.1",
    "istanbul": "github:jacksonrayhamilton/istanbul#09f9660",
    "javascript-brunch": "^2.0.0",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "mocha": "^3.5.3",
    "postcss-brunch": "github:caleb531/postcss-brunch",
    "remap-istanbul": "^0.9.6",
    "rsvg-brunch": "^1.0.0",
    "sass-brunch": "github:caleb531/sass-brunch",
    "sinon": "^2.4.1",
    "sinon-chai": "^2.14.0",
    "sw-precache-brunch": "^2.0.0",
